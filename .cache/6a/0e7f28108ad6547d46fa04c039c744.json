зеров\r\n    &::-webkit-input-placeholder {\r\n      color: $dark-shade_25;\r\n    }\r\n    &::-moz-placeholder {\r\n      color: $dark-shade_25;\r\n    }\r\n  }\r\n\r\n  &__icon-wrapper {\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    width: 44px;\r\n    height: 44px;\r\n    color: $dark-shade_50;\r\n    font-weight: 400;\r\n\r\n  &_subsrciption {\r\n    background-image: $purple_gradiend;\r\n    -webkit-background-clip: text;\r\n    -webkit-text-fill-color: transparent;\r\n  }\r\n\r\n  }\r\n\r\n  // С помощью ' > * ' свойства применяются к дочерним элементам\r\n  &_type_dropdown > * {\r\n    cursor: pointer;\r\n  }\r\n\r\n  &_input-type_dropdown_expanded {\r\n    border-radius: 4px 4px 0 0;\r\n    border: 1px solid $dark-shade_50;\r\n  }\r\n}\r\n\r\n","..\\theme\\vars.scss":"\r\n\r\n//Colors------------------------------------------------------------------\r\n$dark-shade_100: #1F2041;\r\n$dark-shade_75: rgba(31, 32, 65, 0.75);\r\n$dark-shade_50: rgba(31, 32, 65, 0.5);\r\n$dark-shade_25: rgba(31, 32, 65, 0.25);\r\n$dark-shade_5: rgba(31, 32, 65, 0.05);\r\n$purple: #BC9CFF;\r\n$green: #6FCF97;\r\n$body-color: #fff;\r\n$purple_gradiend: linear-gradient(180deg, #BC9CFF 0%, #8BA4F9 100%);\r\n"},"lineCount":null}},"error":null,"hash":"dc74cb007a1d69d15d3068085b5c42b0","cacheData":{"env":{}}}