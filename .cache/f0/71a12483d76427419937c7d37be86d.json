at\";","styles\\reset.scss":"html {\r\n    box-sizing: border-box;\r\n}\r\n\r\n*,\r\n*::after,\r\n*::before {\r\n    box-sizing: inherit;\r\n}\r\n\r\nul[class],\r\nol[class] {\r\n    padding: 0;\r\n}\r\n\r\nbody,\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6,\r\np,\r\nul,\r\nol,\r\nli,\r\nfigure,\r\nblockquote,\r\ndl,\r\ndd {\r\n    margin: 0;\r\n}\r\n\r\nul {\r\n    list-style: none;\r\n}\r\n\r\nimg {\r\n    max-width: 100%;\r\n    display: block;\r\n}\r\n\r\ninput,\r\nbutton,\r\ntextarea,\r\nselect {\r\n    font: inherit;\r\n}","styles\\global.scss":"@mixin font-properties($family, $size, $weight, $height) {\r\n    font-family: $family;\r\n    font-size: $size;\r\n    font-weight: $weight;\r\n    line-height: $height;\r\n}\r\n\r\nhtml,\r\nbody {\r\n    height: 100%;\r\n}\r\n\r\n.wrapper {\r\n    display: flex;\r\n    flex-direction: column;\r\n    min-height: 100%;\r\n}\r\n\r\n.main {\r\n    flex: 1 0 auto;\r\n}\r\n\r\n.footer {\r\n    flex: 0 0 auto;\r\n}\r\n\r\n.container {\r\n    max-width: 1150 px;\r\n    margin: 0 auto;\r\n    padding: 0 15px;\r\n    width: 750px;\r\n}\r\n\r\nh1 {\r\n    @include font-properties($default-font, 24px, 700, 30px)\r\n}\r\n\r\nh2 {\r\n    @include font-properties($default-font, 19px, 700, 23px)\r\n}\r\n\r\nh3 {\r\n    @include font-properties($default-font, 12px, 700, 15px)\r\n}\r\n\r\nbody {\r\n    @include font-properties($default-font, 14px, 400, 24px)\r\n}","styles\\vars.scss":"$default-font: \"Montserrat\", sans-serif;\r\n\r\n//Colors------------------------------------------------------------------\r\n$dark-shade_100: #1F2041;\r\n$dark-shade_75: rgba(31, 32, 65, 0.75);\r\n$dark-shade_50: rgba(31, 32, 65, 0.5);\r\n$dark-shade_25: rgba(31, 32, 65, 0.25);\r\n$dark-shade_5: rgba(31, 32, 65, 0.05);\r\n$purple: #BC9CFF;\r\n$green: #6FCF97;\r\n$body-color: #fff;\r\n$btn-color: linear-gradient(180deg, #BC9CFF 0%, #8BA4F9 100%);\r\n","UI-kit\\Form-Elements\\btn\\btn.scss":" .btn {\r\n    display: inline-block;\r\n    padding: 15px 20px;\r\n    border-radius: 22px;\r\n    font-size: 12px;\r\n    line-height: 15px;\r\n    font-weight: 700;\r\n    text-transform: uppercase;\r\n    cursor: pointer;\r\n    border: 2px solid transparent;\r\n\r\n      &_fullColor {\r\n        border: none;\r\n        background: $btn-color;\r\n        color: #fff;\r\n        background-origin: border-box;\r\n        &:hover {\r\n          opacity: 50%;\r\n        }\r\n      }\r\n      \r\n      &_borderColor {\r\n        position: relative;\r\n        padding: 13px 18px;\r\n        background-clip: padding-box;\r\n        color: #BC9CFF;\r\n        background-color: $body-color;\r\n        &::before {\r\n          content: '';\r\n          position: absolute;\r\n          top: 0; right: 0; bottom: 0; left: 0;\r\n          z-index: -1;\r\n          margin: -2px;\r\n          border-radius: inherit;\r\n          background: $btn-color;\r\n          }\r\n          &:hover::before {\r\n            opacity: 50%;\r\n        }\r\n      }\r\n      \r\n      &_withoutBorder {\r\n        padding: 0;\r\n        border: none;\r\n        background-color: $body-color;\r\n        color: #BC9CFF;\r\n        &:hover {\r\n          color: #1F2041;\r\n          opacity: .5;\r\n        }\r\n      }\r\n\r\n      &_submit {\r\n        position: relative;\r\n        border: none;\r\n        display: flex;\r\n        background: $btn-color;\r\n        background-clip: padding-box;\r\n        color: #fff;\r\n        align-items: center;\r\n        width: 320px;\r\n        height: 44px;\r\n      }\r\n      &__title_submit {\r\n        margin: 0 auto;\r\n      }\r\n      &_arrow {\r\n        position: absolute;\r\n        right: 0;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        width: 44px;\r\n        height: 44px;\r\n      }\r\n}\r\n\r\n\r\n\r\n\r\n","UI-kit\\Form-Elements\\text-field\\text-field.scss":"@mixin text-field_size_s {\r\n    width: 150px;\r\n    height: 44px;\r\n}\r\n\r\n@mixin text-field_size_m {\r\n    width: 267px;\r\n    height: 44px;\r\n}\r\n\r\n@mixin text-field_size_l {\r\n    width: 320px;\r\n    height: 44px;\r\n}\r\n\r\n@mixin text-field {\r\n    .text-field {\r\n        display: block;\r\n        color: $dark-shade_75;;\r\n        padding-left: 15px;\r\n        border-radius: 4px;\r\n        border: 1px solid $dark-shade_25;\r\n        &:hover, &:focus {\r\n            border: 1px solid $dark-shade_50;\r\n        }\r\n    }\r\n}\r\n\r\n@mixin text-field_default {\r\n    .text-field {\r\n          &_default {\r\n            @include text-field_size_l;\r\n          }\r\n        //окрашиваем цвет placeholder для всех браузеров\r\n          &_default::-webkit-input-placeholder {\r\n              color: $dark-shade_25;\r\n          }\r\n          &_default::-moz-placeholder {\r\n              color: $dark-shade_25;\r\n          }\r\n    }\r\n}\r\n\r\n@mixin text-field_dropdown {\r\n    .text-field {\r\n        &_dropdown {\r\n            @include text-field_size_l;\r\n            position: relative;\r\n            display: flex;\r\n            align-items: center;\r\n        }\r\n        &__expend-icon {\r\n            position: absolute;\r\n            right: 0;\r\n            display: flex;\r\n            justify-content: center;\r\n            align-items: center;\r\n            width: 44px;\r\n            height: 44px;\r\n        }\r\n    }\r\n}\r\n@mixin text-field_dropdown_date {\r\n    .text-field {\r\n        &_dropdown_date {\r\n            @include text-field_size_s;\r\n        }\r\n    }\r\n}\r\n\r\n@mixin text-field_dropdown_filter-date {\r\n    .text-field {\r\n        &_dropdown_filter-date {\r\n            @include text-field_size_m;\r\n        }\r\n    }\r\n}\r\n\r\n","UI-kit\\colors&type\\colors&type.scss":".wrapper-color-n-text {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    margin: 125px;\r\n}\r\n\r\n.color-box {\r\n    width: 70px;\r\n    height: 70px;\r\n    borde