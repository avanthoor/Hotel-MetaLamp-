wap\");\r\n\r\n@import url(\"https://fonts.googleapis.com/css?family=Quicksand:700&display=swap\");\r\n\r\nh1 {\r\n    @include font-properties($default-font, 24px, 700, 30px)\r\n}\r\n\r\nh2 {\r\n    @include font-properties($default-font, 19px, 700, 23px)\r\n}\r\n\r\nh3 {\r\n    @include font-properties($default-font, 12px, 700, 15px)\r\n}\r\n\r\nbody {\r\n    @include font-properties($default-font, 14px, 400, 24px)\r\n}\r\n\r\n//                        RESETS\r\n//------------------------------------------------------------------\r\nhtml {\r\n    box-sizing: border-box;\r\n}\r\n\r\n*,\r\n*::after,\r\n*::before {\r\n    box-sizing: inherit;\r\n}\r\n\r\nul[class],\r\nol[class] {\r\n    padding: 0;\r\n}\r\n\r\nbody,\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6,\r\np,\r\nul,\r\nol,\r\nli,\r\nfigure,\r\nblockquote,\r\ndl,\r\ndd {\r\n    margin: 0;\r\n}\r\n\r\nul {\r\n    list-style: none;\r\n}\r\n\r\nimg {\r\n    max-width: 100%;\r\n    display: block;\r\n}\r\n\r\ninput,\r\nbutton,\r\ntextarea,\r\nselect {\r\n    font: inherit;\r\n}","..\\components\\btn\\btn.scss":"@import '../../theme/vars.scss';\r\n@import '../../theme/global.scss';\r\n \r\n .btn {\r\n    display: inline-block;\r\n    padding: 15px 20px;\r\n    border-radius: 22px;\r\n    font-size: 12px;\r\n    line-height: 15px;\r\n    font-weight: 700;\r\n    text-transform: uppercase;\r\n    cursor: pointer;\r\n    border: 2px solid transparent;\r\n\r\n      &_color-full {\r\n        border: none;\r\n        background: $purple_gradiend;\r\n        color: #fff;\r\n        background-origin: border-box;\r\n        &:hover {\r\n          opacity: 50%;\r\n        }\r\n      }\r\n      \r\n      &_color-only-border {\r\n        position: relative;\r\n        padding: 13px 18px;\r\n        background-clip: padding-box;\r\n        color: $purple;\r\n        background-color: $body-color;\r\n        &::before {\r\n          content: '';\r\n          position: absolute;\r\n          top: 0; right: 0; bottom: 0; left: 0;\r\n          z-index: -1;\r\n          margin: -2px;\r\n          border-radius: inherit;\r\n          background: $purple_gradiend;\r\n          }\r\n          &:hover::before {\r\n            opacity: 50%;\r\n        }\r\n      }\r\n      \r\n      &_without-border {\r\n        padding: 0;\r\n        border: none;\r\n        background-color: $body-color;\r\n        color: $purple;\r\n        &:hover {\r\n          color: #1F2041;\r\n          opacity: .5;\r\n        }\r\n      }\r\n\r\n      &_submit {\r\n        position: relative;\r\n        border: none;\r\n        background: $purple_gradiend;\r\n        background-clip: padding-box;\r\n        color: #fff;\r\n        width: 320px;\r\n        height: 44px;\r\n        text-align: center;\r\n      }\r\n\r\n      &__icon-wrapper {\r\n        position: absolute;\r\n        top: 0;\r\n        right: 0;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        width: 44px;\r\n        height: 44px;\r\n        color: #fff;\r\n        font-weight: 400;\r\n      }\r\n      \r\n}\r\n\r\n\r\n\r\n\r\n","..\\theme\\vars.scss":"\r\n\r\n//Colors------------------------------------------------------------------\r\n$dark-shade_100: #1F2041;\r\n$dark-shade_75: rgba(31, 32, 65, 0.75);\r\n$dark-shade_50: rgba(31, 32, 65, 0.5);\r\n$dark-shade_25: rgba(31, 32, 65, 0.25);\r\n$dark-shade_5: rgba(31, 32, 65, 0.05);\r\n$purple: #BC9CFF;\r\n$green: #6FCF97;\r\n$body-color: #fff;\r\n$purple_gradiend: linear-gradient(180deg, #BC9CFF 0%, #8BA4F9 100%);\r\n"},"lineCount":null}},"error":null,"hash":"7d93e48facdc88fe5935e8895d850e7e","cacheData":{"env":{}}}